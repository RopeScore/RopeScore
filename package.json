{
  "name": "RopeScore",
  "productName": "RopeScore",
  "version": "2.0.0-au10",
  "description": "A scoring system for ropeskipping",
  "author": "Svante Bengtson <ropescore@swant.pw>",
  "main": "main.js",
  "postinstall": "install-app-deps",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.15.2",
    "express-ws": "^3.0.0",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "electron": "^1.6.2",
    "electron-packager": "^8.7.1",
    "electron-winstaller": "^2.6.2",
    "yargs": "^8.0.1"
  },
  "scripts": {
    "start": "electron .",
    "build": "node build",
    "build:win": "node build -p win32",
    "build:win:64": "node build -p win32 -a x64",
    "build:win:32": "node build -p win32 -a ia32",
    "build:mac": "node build -p darwin",
    "build:linux": "node build -p linux",
    "build:linux:64": "node build -p linux -a x64",
    "build:linux:32": "node build -p linux -a ia32",
    "build:linux:arm": "node build -p linux -a armv7l",
    "build:all": "node build --all",
    "dist:sync": "rsync -avzcP dist/win32 lhome:"
  }
}
