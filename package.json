{
  "name": "RopeScore",
  "productName": "RopeScore",
  "version": "2.3.0",
  "description": "A scoring system for ropeskipping",
  "author": "Svante Bengtson",
  "repository": "svbeon/RopeScore",
  "bugs": {
    "email": "contact@ropescore.com"
  },
  "main": "main.js",
  "postinstall": "install-app-deps",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.15.2",
    "express-ws": "^3.0.0",
    "fs-extra": "^3.0.1"
  },
  "devDependencies": {
    "archiver": "^2.0.3",
    "electron": "^1.7.10",
    "electron-packager": "^8.7.1",
    "electron-winstaller": "^2.6.2",
    "protractor": "^5.2.0",
    "yargs": "^8.0.1"
  },
  "scripts": {
    "start": "electron .",
    "build": "node build",
    "build:all:mac": "node build -a x64 -p darwin && node build -a x64 -c se -p darwin && node build -a x64 -c au -p darwin",
    "build:all": "node build -a x64 && node build -a x64 -c se && node build -a x64 -c au && node build -a ia32 && node build -a ia32 -c se && node build -a ia32 -c au",
    "dist:sync": "rsync -avzcP --exclude-from .rsyncignore dist/ lhome:ropescore"
  }
}